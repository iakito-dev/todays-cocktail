{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "scripts/run-with-asdf.sh vite",
    "build": "scripts/run-with-asdf.sh vite build",
    "lint": "scripts/run-with-asdf.sh eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "scripts/run-with-asdf.sh eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "scripts/run-with-asdf.sh prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "scripts/run-with-asdf.sh prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "preview": "scripts/run-with-asdf.sh vite preview",
    "sitemap": "scripts/run-with-asdf.sh node scripts/generate-sitemap.mjs",
    "test": "scripts/run-with-asdf.sh node --require ./vitest.polyfill.cjs ./node_modules/vitest/vitest.mjs",
    "test:ui": "scripts/run-with-asdf.sh node --require ./vitest.polyfill.cjs ./node_modules/vitest/vitest.mjs --ui",
    "test:coverage": "scripts/run-with-asdf.sh node --require ./vitest.polyfill.cjs ./node_modules/vitest/vitest.mjs --coverage"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@vercel/analytics": "^1.5.0",
    "@vuer-ai/react-helmet-async": "^2.0.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-istanbul": "^4.0.10",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.5"
  }
